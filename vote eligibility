
public class VoteorNot 
{
	public static void main(String[]args)
	{
		int count=0;
		int len=args.length;
		for(int i=0;i<len;i++)
		{
			String num=args[i];
			int age=Integer.parseInt(num);
			if(age>=18)
			{
				count++;
				System.out.println("you can vote");
			}
			else
			{
				System.out.println("you cannot vote");
			}
			
		}
		System.out.println("Total eligible="+count);
	}


}
